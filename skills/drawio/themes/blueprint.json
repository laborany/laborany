    "process": {
      "fillColor": "bbdefb",
      "strokeColor": "1565c0"
    },
    "decision": {
      "fillColor": "fff9c4",
      "strokeColor": "f9a825"
    },
    "database": {
      "fillColor": "e1bee7",
      "strokeColor": "7b1fa2"
    }
  },

  "edges": {
    "default": {
      "strokeColor": "1565c0",
      "strokeWidth": 2,
      "endArrow": "classic"
    }
  },

  "groups": {
    "default": {
      "fillColor": "e3f2fd",
