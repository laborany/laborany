{
  "name": "blueprint",
  "displayName": "蓝图主题",
  "description": "深蓝技术风格，适合技术架构和系统设计",

  "colors": {
    "primary": "0d47a1",
    "secondary": "1565c0",
    "accent": "1976d2",
    "background": "e3f2fd",
    "surface": "bbdefb",
    "text": "0d47a1",
    "textLight": "1565c0",
